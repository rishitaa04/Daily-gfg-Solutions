7 8 9 10



class Solution{
    static void printFloydTriangle(int N){
        // code here
        int var =1;
        for(int i=1;i<=N;i++){
            for(int j=1;j<=i;j++){
                System.out.print(var++ +" ");
            }
            System.out.println("");
        }
    }
}
