class Solve {
    int[] getMoreAndLess(int[] arr, int n, int x) {
        // code here
       int min = 0;
       int max = 0;
       for(int i = 0 ; i < n ; i++){
           if(arr[i] >= x){
               max++;
           }
           if(arr[i] <= x){
               min++;
           }
       }
        int[] a = {min , max};
           return a;
    }
}
